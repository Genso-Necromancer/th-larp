; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Touhou: Living Adventure of Resonant Play"
run/main_scene="res://scenes/main.tscn"
config/use_custom_user_dir=true
config/custom_user_dir_name="Crypt\\th-larp"
config/features=PackedStringArray("4.4", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Enums="*res://scrpt/global/enums.gd"
SignalTower="*res://scrpt/global/signal_tower.gd"
Options="*res://scrpt/global/Options.gd"
Global="*res://scrpt/global/Global.gd"
PlayerData="*res://scrpt/global/PlayerData.gd"
StringGetter="*res://scrpt/global/string_getter.gd"
GameState="*res://scrpt/ref/game_state.gd"
CutsceneManager="*res://scenes/cutscenes/CutsceneManager.gd"
SaveHub="*res://scrpt/global/save_hub.gd"

[display]

window/size/viewport_width=1920
window/size/viewport_height=1080
window/size/borderless=true
window/stretch/mode="viewport"
window/custom_image=""

[dotnet]

project/assembly_name="The Crypt"

[editor]

movie_writer/mjpeg_quality=0.5
movie_writer/movie_file="C:/Users/Necromancer/Documents/game_clips/toss_em.avi"

[editor_plugins]

enabled=PackedStringArray("res://addons/CodeEditorBackground/plugin.cfg")

[global_group]

Unit="All game Units"
Persist=""

[gui]

theme/custom="res://fonts/Izayoi_Theme.tres"

[importer_defaults]

texture={
"compress/channel_pack": 0,
"compress/hdr_compression": 1,
